{
  "name": "indexers",
  "version": "1.0.0",
  "description": "apibara indexers to index DeFi Spring $STRK claim data",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "get_synced_block": "tsx src/get_synced_block.ts",
    "process_contracts": "ts-node src/processContracts",
    "start": "apibara run --allow-env=.env src/indexer.ts --sink-id=1 --status-server-address=0.0.0.0:10000",
    "sync": "apibara run --allow-env=.env src/sync.ts --sink-id=105 --status-server-address=0.0.0.0:1234",
    "start-nimbora": "apibara run --allow-env=.env src/nimbora_indexer.ts --sink-id=2 --status-server-address=0.0.0.0:1234",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "akiraonstarknet",
  "license": "ISC",
  "dependencies": {
    "@apibara/protocol": "0.4.9",
    "@apibara/starknet": "0.4.0",
    "@prisma/client": "5.12.1",
    "dotenv": "16.4.5",
    "prisma": "^5.12.1",
    "starknet": "^6.7.0",
    "tslib": "^2.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "browserify": "^17.0.0",
    "eslint": "^9.0.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "sucrase": "^3.35.0",
    "ts-node": "10.9.1",
    "tsify": "^5.0.4",
    "tsx": "^4.9.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.6.0"
  }
}
